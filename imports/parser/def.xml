<!--
/^[\s]*<question [\s]*type[\s]*=[\s]*"(VF)">[\s]*<text>[\s]*(.*)[\s]*<\/text>[\s]*<answer [\s]*isRight[\s]*=[\s]*"(yes|no)"[\s]*>(.*)<\/answer>[\s]*<\/question>[\s]*$/;
-->
<question type="VF">
	<text>
		testo della domanda
	</text>
	<answer isRight="yes"></answer>
</question>

<!--
^[\s]*<question [\s]*type[\s]*=[\s]*"(MU)">[\s]*<text>[\s]*(.*)[\s]*<\/text>(([\s]*<answer [\s]*isRight[\s]*=[\s]*"(yes|no)"[\s]*>(.*)<\/answer>[\s]*){2,})<\/question>[\s]*$
-->
<question type="MU">
    <text>
    	testo della domanda
    </text>
    <answer isRight="yes"> risp1 </answer>
    <answer isRight="no"> risp2 </answer>
    <answer isRight="no"> risp3 </answer>
</question>


<!--
^[\s]*<question [\s]*type[\s]*=[\s]*"(MX)">[\s]*<text>[\s]*(.*)[\s]*<\/text>(([\s]*<answer [\s]*isRight[\s]*=[\s]*"(yes|no)"[\s]*>(.*)<\/answer>[\s]*){2,})<\/question>[\s]*$
-->
<question type="MX"> 
	<text>
    	testo della domanda
    </text>
    <answer isRight="yes"> risp1 </answer>
    <answer isRight="yes"> risp2 </answer>
    <answer isRight="no"> risp3 </answer>
</question>


<!--
^[\s]*<question [\s]*type[\s]*=[\s]*"(AS)">[\s]*<text>[\s]*(.*)[\s]*<\/text>[\s]*((<answer [\s]*set[\s]*=[\s]*"(A|B)" [\s]*pos[\s]*=[\s]*"(\d)"[\s]*>(.*)<\/answer>[\s]*){2,})<\/question>[\s]*$
-->
<question type="AS"> 
	<text>
    	testo della domanda
    </text>
    <answer set="A" pos="1"> risp </answer>
    <answer set="A" pos="2"> risp </answer>
    <answer set="B" pos="1"> risp </answer>
    <answer set="B" pos="2"> risp </answer>
    <answer set="B" pos="3"> risp </answer>
</question>

<!--
^[\s]*<question [\s]*type[\s]*=[\s]*"(OD)">[\s]*<text>[\s]*(.*)[\s]*<\/text>[\s]*((<answer [\s]*pos[\s]*=[\s]*"(\d)"[\s]*>(.*)<\/answer>[\s]*){2,})<\/question>[\s]*$
-->
<question type="OD">
	<text>
    	testo della domanda
    </text>
    <answer pos="1"> risp </answer>
    <answer pos="2"> risp </answer>
    <answer pos="3"> risp </answer>
    <answer pos="4"> risp </answer>
    <answer pos="5"> risp </answer>
</question>
